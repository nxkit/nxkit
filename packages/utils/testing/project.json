{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/utils/testing/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/utils/testing",
        "main": "packages/utils/testing/src/index.ts",
        "tsConfig": "packages/utils/testing/tsconfig.lib.json",
        "assets": ["packages/utils/testing/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/utils/testing/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/utils/testing"],
      "options": {
        "jestConfig": "packages/utils/testing/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
